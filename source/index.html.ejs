                                    target='_blank'
                                )
                                    | Curriculum Vitae
                                    != `<!--${ALTERNATE_LANGUAGE}:`
                                    | Lebenslauf
                                    != '-->'
                                a(href=LINK_V_CARD_PATH, target='_blank') vCard
                    if (section.title === 'contact')
