    // region helper functions
    swapCase = (letters) => {
        var newLetters = ''
        for (var index = 0; index < letters.length; index++)
            if (letters[index] === letters[index].toLowerCase())
                newLetters += letters[index].toUpperCase()
            else
                newLetters += letters[index].toLowerCase()
        return newLetters
    }
