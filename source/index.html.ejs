    body.no-javascript.home-page.tools-hidden-on-javascript-enabled
        script(type="text/javascript").
            window.document.body.className =
                window.document.body.className.replace(
                    /(^|\s)no-javascript(\s|$)/, '$1javascript$2');
        .website-window-loading-cover.tools-visible-on-javascript-enabled: div
        // region menu
        header.navbar-wrapper(
            class=`website-start-up-animation-number-${START_UP_ANIMATION_NUMBER}`
        )
            .container: .navbar.navbar-inverse.navbar-static-top: .container
                .navbar-header
                    a.navbar-brand(href='#')
                        | thaibault
                        = ' '
                        span.dimension-indicator
                    - START_UP_ANIMATION_NUMBER += 1
                    a.tools-visible-on-javascript-enabled(
                        href=`#language-${ALTERNATE_LANGUAGE}`
                        class=`website-start-up-animation-number-${START_UP_ANIMATION_NUMBER}`
                    ) #{ALTERNATE_LANGUAGE.substring(0, ALTERNATE_LANGUAGE.length - 2)}
                    button.navbar-toggle.tools-visible-on-javascript-enabled(
                        type='button' data-toggle='collapse'
                        data-target='.navbar-collapse'
                    )
                        each section in [1, 2, 3]
                            span.icon-bar
                .navbar-collapse.collapse
                    .navbar-highlighter.tools-visible-on-javascript-enabled
                    ul.nav.navbar-nav
                        each section in SECTIONS
                            if (section.content[0])
                                - START_UP_ANIMATION_NUMBER += 1
                                li(
                                    class=`website-start-up-animation-number-${START_UP_ANIMATION_NUMBER}${(section.title === SECTIONS[0].title ? ' active' : '')}`
                                )
                                    a(href=`#${section.title}`)
                                        | #{section.content[0][DEFAULT_LANGUAGE]}
                                        != `<!--${ALTERNATE_LANGUAGE}:${section.content[0][ALTERNATE_LANGUAGE]}-->`
        // endregion
        // region carousel
        section.carousel.slide: .carousel-inner
            each section in SECTIONS
                .item(class=section.title): div(
                    class=`carousel-image-${section.title}`
                ): .container
                    if (section.content[1][DEFAULT_LANGUAGE])
                        h1(id=section.title)
                            | #{section.content[1][DEFAULT_LANGUAGE]}
                            != `<!--${ALTERNATE_LANGUAGE}:${section.content[1][ALTERNATE_LANGUAGE]}-->`
                    else
                        // Serves as anchor point if javaScript isn't supported.
                        h1(id=section.title)
                    if (section.title === 'work')
                        h2
                            | #{section.content[2][DEFAULT_LANGUAGE]}
                            != `<!--${ALTERNATE_LANGUAGE}:${section.content[2][ALTERNATE_LANGUAGE]}-->`
                        p
                            langReplace
                                each keywords in section.content[3][DEFAULT_LANGUAGE]
                                    if (keywords[0] !== section.content[3][DEFAULT_LANGUAGE][0][0])
                                        = ' '
                                        strong #{keywords[0]}
                                        = ' '
                                    | #{keywords[1].join(', ')}
                            != `<!--${ALTERNATE_LANGUAGE}:`
                            each keywords in section.content[3][ALTERNATE_LANGUAGE]
                                if (keywords[0] !== section.content[3][ALTERNATE_LANGUAGE][0][0])
                                    strong #{keywords[0]}
                                | #{keywords[1].join(', ')}
                            != '-->'
                        h2
                            | #{section.content[4][DEFAULT_LANGUAGE]}
                            != `<!--${ALTERNATE_LANGUAGE}:`
                            | #{section.content[4][ALTERNATE_LANGUAGE]}
                            != '-->'
                        dl.dl-horizontal
                            each project in PROJECTS
                                dt: a(href=project.url, target='_blank').
                                    #{project.title}
                                dd
                                    | #{project.content[DEFAULT_LANGUAGE][0]}
                                    != `<!--${ALTERNATE_LANGUAGE}:`
                                    | #{project.content[ALTERNATE_LANGUAGE][0]}
                                    != '-->'
                                    if (project.content[DEFAULT_LANGUAGE].length > 1)
                                        ul
                                            - index = 0
                                            each element in project.content[DEFAULT_LANGUAGE][1]
                                                li
                                                    if (Array.isArray(element))
                                                        langreplace: a(
                                                            href=element[1]
                                                        ) #{element[0]}
                                                        langreplacement
                                                            | #{ALTERNATE_LANGUAGE}:
                                                            a(
                                                                href=project.content[ALTERNATE_LANGUAGE][1][index][1]
                                                            ) #{project.content[ALTERNATE_LANGUAGE][1][index][0]}
                                                    else
                                                        | #{element}
                                                        != `<!--${ALTERNATE_LANGUAGE}:`
                                                        | #{project.content[ALTERNATE_LANGUAGE][1][index]}
                                                        != '-->'
                                                - index += 1
                    else
                        p.lead
                            | #{section.content[2][DEFAULT_LANGUAGE]}
                            != `<!--${ALTERNATE_LANGUAGE}:`
                            | #{section.content[2][ALTERNATE_LANGUAGE]}
                            != '-->'
                            if (section.title === 'about')
                                a.visible-xs(
                                    href=LINK_CURRICULUM_VITAE_DEFAULT_LANGUAGE_PATH
                                    target='_blank'
                                )
                                    | Curriculum Vitae (German)
                                    != `<!--${ALTERNATE_LANGUAGE}:`
                                    | Lebenslauf (Deutsch)
                                    != '-->'
                                a.visible-xs(
